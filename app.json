{
  "expo": {
    "name": "e-tanzeem-diary",
    "slug": "e-tanzeem-diary",
    "owner": "jiislamabad",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "etanzeemdiary",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jiislamabad.etanzeemdiaryapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jiislamabad.etanzeemdiaryapp",
      "googleServicesFile": "./google-services.json"

    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-notifications", 
        [
          "expo-splash-screen",
          {
            "image": "./assets/images/splash-icon.png",
            "imageWidth": 1563,
            "imageHeight": 3383,
            "resizeMode": "contain",
            "backgroundColor": "#008CFF"
          }
        ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/JameelNooriNastaleeq.ttf",
            "./assets/fonts/noori-kasheed.ttf"
          ]
        }
      ],
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "initialRouteName": "splash",
      "eas": {
        "projectId": "c94ec490-9e85-465c-bc59-cc7825b5312a",
        "owner": "jiislamabad"
      },
      "EXPO_PUBLIC_API_BASE_URL": "https://admin.jiislamabad.org"
    },
    "assets": [
      "./assets/images/splash-icon.png"
    ]
  }
}